class TriangleOne {
	public static void main(String[] args) {
		for(int i = 0; i < 3; i++){
			for(int j = 0; j < 3; j++){
				if (i >= j) {
					System.out.print("* ");
				}
			}
			System.out.println();
		}
	}
}

-----------------------------------------------------------------------------------

class TriangleTwo {
	public static void main(String[] args) {
		for(int i = 0; i < 3; i++){
			for(int j = 0; j < 3; j++){
				if (i > j) {
					System.out.print("  ");
				}
				else {
					System.out.print("* ");
				}
			}
			System.out.println();
		}
	}
}

-----------------------------------------------------------------------------------

class TriangleThree {
	public static void main(String[] args) {
		for(int i = 0; i < 3; i++){
			for(int j = 2; j >= 0; j--){
				if (i >= j) {
					System.out.print("* ");
				}
				else {
					System.out.print("  ");
				}
			}
			System.out.println();
		}
	}
}
